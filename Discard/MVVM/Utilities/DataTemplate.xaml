    xmlns:vm="clr-namespace:Client.MVVM.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate DataType="{x:Type vm:MessagesVM}">
        <pages:Messages />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:WelcomeVM}">
        <pages:WelcomePage />
    </DataTemplate>

</ResourceDictionary>